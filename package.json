{
  "name": "sdkwork-browser-agent",
  "version": "1.0.0",
  "description": "Browser-compatible agent architecture with Skills, MCP, Tools, evaluation system, and flexible LLM Provider support",
  "type": "module",
  "main": "dist/node/index.cjs",
  "module": "dist/node/index.js",
  "types": "dist/node/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/node/index.d.ts",
      "browser": {
        "types": "./dist/browser/index.d.ts",
        "import": "./dist/browser/index.js"
      },
      "node": {
        "types": "./dist/node/index.d.ts",
        "import": "./dist/node/index.js",
        "require": "./dist/node/index.cjs"
      },
      "default": {
        "types": "./dist/node/index.d.ts",
        "import": "./dist/node/index.js",
        "require": "./dist/node/index.cjs"
      }
    },
    "./llm": {
      "types": "./dist/node/llm/index.d.ts",
      "browser": {
        "types": "./dist/browser/llm/index.d.ts",
        "import": "./dist/browser/llm/index.js"
      },
      "node": {
        "types": "./dist/node/llm/index.d.ts",
        "import": "./dist/node/llm/index.js",
        "require": "./dist/node/llm/index.cjs"
      }
    },
    "./skills": {
      "types": "./dist/node/skills/index.d.ts",
      "browser": {
        "types": "./dist/browser/skills/index.d.ts",
        "import": "./dist/browser/skills/index.js"
      },
      "node": {
        "types": "./dist/node/skills/index.d.ts",
        "import": "./dist/node/skills/index.js",
        "require": "./dist/node/skills/index.cjs"
      }
    },
    "./tools": {
      "types": "./dist/node/tools/index.d.ts",
      "browser": {
        "types": "./dist/browser/tools/index.d.ts",
        "import": "./dist/browser/tools/index.js"
      },
      "node": {
        "types": "./dist/node/tools/index.d.ts",
        "import": "./dist/node/tools/index.js",
        "require": "./dist/node/tools/index.cjs"
      }
    },
    "./mcp": {
      "types": "./dist/node/mcp/index.d.ts",
      "browser": {
        "types": "./dist/browser/mcp/index.d.ts",
        "import": "./dist/browser/mcp/index.js"
      },
      "node": {
        "types": "./dist/node/mcp/index.d.ts",
        "import": "./dist/node/mcp/index.js",
        "require": "./dist/node/mcp/index.cjs"
      }
    },
    "./storage": {
      "types": "./dist/node/storage/index.d.ts",
      "browser": {
        "types": "./dist/browser/storage/index.d.ts",
        "import": "./dist/browser/storage/index.js"
      },
      "node": {
        "types": "./dist/node/storage/index.d.ts",
        "import": "./dist/node/storage/index.js",
        "require": "./dist/node/storage/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:browser": "tsup --config tsup.config.ts browserConfig",
    "build:node": "tsup --config tsup.config.ts nodeConfig",
    "build:watch": "tsup --watch",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:file": "vitest run",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "clean": "rm -rf dist",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "verify": "npm run lint && npm run typecheck && npm run test && npm run build"
  },
  "keywords": [
    "agent",
    "ai",
    "skills",
    "mcp",
    "tools",
    "llm",
    "browser",
    "typescript",
    "openai",
    "claude",
    "gemini"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jsdom": "^27.4.0",
    "prettier": "^3.1.1",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitepress": "^1.6.4",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
